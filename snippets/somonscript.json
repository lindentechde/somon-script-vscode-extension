{
  "Function Declaration": {
    "prefix": "функсия",
    "body": [
      "функсия ${1:номи_функсия}(${2:параметрҳо}) {",
      "\t${3:// Коди функсия}",
      "\tбозгашт ${4:натиҷа};",
      "}"
    ],
    "description": "Эълон кардани функсия / Function declaration"
  },
  "Async Function": {
    "prefix": "ҳамзамон",
    "body": [
      "ҳамзамон функсия ${1:номи_функсия}(${2:параметрҳо}) {",
      "\t${3:интизор} ${4:амалиёт};",
      "\tбозгашт ${5:натиҷа};",
      "}"
    ],
    "description": "Функсияи асинхронӣ / Async function"
  },
  "If Statement": {
    "prefix": "агар",
    "body": [
      "агар (${1:шарт}) {",
      "\t${2:// Коди шарт}",
      "} вагарна {",
      "\t${3:// Коди дигар}",
      "}"
    ],
    "description": "Изҳороти шартӣ / If statement"
  },
  "For Loop": {
    "prefix": "барои",
    "body": [
      "барои (тағйирёбанда ${1:i} = ${2:0}; ${1:i} < ${3:10}; ${1:i}++) {",
      "\t${4:// Коди давра}",
      "}"
    ],
    "description": "Давраи барои / For loop"
  },
  "Class Declaration": {
    "prefix": "синф",
    "body": [
      "синф ${1:НомиСинф} {",
      "\tконструктор(${2:параметрҳо}) {",
      "\t\tин.${3:хосият} = ${2:параметрҳо};",
      "\t}",
      "",
      "\t${4:метод}() {",
      "\t\t${5:// Коди метод}",
      "\t}",
      "}"
    ],
    "description": "Эълони синф / Class declaration"
  },
  "Interface Declaration": {
    "prefix": "интерфейс",
    "body": [
      "интерфейс ${1:НомиИнтерфейс} {",
      "\t${2:хосият}: ${3:навъ};",
      "\t${4:метод}(${5:параметрҳо}): ${6:навъи_бозгашт};",
      "}"
    ],
    "description": "Эълони интерфейс / Interface declaration"
  },
  "Variable Declaration": {
    "prefix": "тағйирёбанда",
    "body": [
      "тағйирёбанда ${1:номи_тағйирёбанда} = ${2:қимат};"
    ],
    "description": "Эълони тағйирёбанда / Variable declaration"
  },
  "Constant Declaration": {
    "prefix": "собит",
    "body": [
      "собит ${1:НОМ_СОБИТ} = ${2:қимат};"
    ],
    "description": "Эълони собит / Constant declaration"
  },
  "Console Log": {
    "prefix": "чоп",
    "body": [
      "чоп.сабт(${1:паём});"
    ],
    "description": "Чопи паём / Console log"
  },
  "Try Catch": {
    "prefix": "кӯшиш",
    "body": [
      "кӯшиш {",
      "\t${1:// Код бо эҳтимоли хато}",
      "} гирифтан (${2:хато}) {",
      "\tчоп.хато(${2:хато});",
      "} ниҳоят {",
      "\t${3:// Коди ниҳоӣ}",
      "}"
    ],
    "description": "Блоки кӯшиш-гирифтан / Try-catch block"
  },
  "Import Statement": {
    "prefix": "ворид",
    "body": [
      "ворид { ${1:номҳо} } аз '${2:./модул}';"
    ],
    "description": "Изҳороти воридот / Import statement"
  },
  "Export Default": {
    "prefix": "содир",
    "body": [
      "содир пешфарз ${1:чизе};"
    ],
    "description": "Содироти пешфарз / Export default"
  },
  "Arrow Function": {
    "prefix": "=>",
    "body": [
      "(${1:параметрҳо}) => {",
      "\t${2:// Коди функсия}",
      "}"
    ],
    "description": "Функсияи тир / Arrow function"
  },
  "Switch Statement": {
    "prefix": "интихоб",
    "body": [
      "интихоб (${1:ифода}) {",
      "\tҳолат ${2:қимат1}:",
      "\t\t${3:// Код}",
      "\t\tшикастан;",
      "\tҳолат ${4:қимат2}:",
      "\t\t${5:// Код}",
      "\t\tшикастан;",
      "\tпешфарз:",
      "\t\t${6:// Коди пешфарз}",
      "}"
    ],
    "description": "Изҳороти интихоб / Switch statement"
  },
  "Type Definition": {
    "prefix": "навъ",
    "body": [
      "навъ ${1:НомиНавъ} = ${2:таъриф};"
    ],
    "description": "Таъриф додани навъ / Type definition"
  },
  "Namespace": {
    "prefix": "номфазо",
    "body": [
      "номфазо ${1:НомиНомфазо} {",
      "\t${2:// Эълонҳо}",
      "}"
    ],
    "description": "Номфазо / Namespace"
  },
  "Generic Function": {
    "prefix": "функсия<T>",
    "body": [
      "функсия ${1:ном}<${2:T}>(${3:параметр}: ${2:T}): ${4:${2:T}} {",
      "\t${5:// Коди функсия}",
      "\tбозгашт ${6:параметр};",
      "}"
    ],
    "description": "Функсияи умумӣ / Generic function"
  },
  "Generic Class": {
    "prefix": "синф<T>",
    "body": [
      "синф ${1:НомиСинф}<${2:T}> {",
      "\tхосусӣ ${3:қимат}: ${2:T};",
      "",
      "\tконструктор(${3:қимат}: ${2:T}) {",
      "\t\tин.${3:қимат} = ${3:қимат};",
      "\t}",
      "",
      "\tфунксия гирифтан(): ${2:T} {",
      "\t\tбозгашт ин.${3:қимат};",
      "\t}",
      "}"
    ],
    "description": "Синфи умумӣ / Generic class"
  },
  "While Loop": {
    "prefix": "то",
    "body": [
      "то (${1:шарт}) {",
      "\t${2:// Коди давра}",
      "}"
    ],
    "description": "Давраи то (while) / While loop"
  },
  "Do-While Loop": {
    "prefix": "кардан-то",
    "body": [
      "кардан {",
      "\t${1:// Коди давра}",
      "} то (${2:шарт});"
    ],
    "description": "Давраи кардан-то / Do-while loop"
  },
  "For-In Loop": {
    "prefix": "барои-дар",
    "body": [
      "барои (тағйирёбанда ${1:калид} дар ${2:объект}) {",
      "\t${3:// Коди давра}",
      "}"
    ],
    "description": "Давраи барои-дар / For-in loop"
  },
  "For-Of Loop": {
    "prefix": "барои-аз",
    "body": [
      "барои (тағйирёбанда ${1:элемент} аз ${2:массив}) {",
      "\t${3:// Коди давра}",
      "}"
    ],
    "description": "Давраи барои-аз / For-of loop"
  },
  "Template Literal": {
    "prefix": "``",
    "body": [
      "`${1:матн} \\${${2:ифода}} ${3:матн}`"
    ],
    "description": "Литерали шаблонӣ / Template literal"
  },
  "Arrow Function": {
    "prefix": "тир",
    "body": [
      "(${1:параметрҳо}) => ${2:ифода}"
    ],
    "description": "Функсияи тир / Arrow function"
  },
  "Arrow Function Block": {
    "prefix": "тир-блок",
    "body": [
      "(${1:параметрҳо}) => {",
      "\t${2:// Коди функсия}",
      "\tбозгашт ${3:натиҷа};",
      "}"
    ],
    "description": "Функсияи тир бо блок / Arrow function with block"
  },
  "Ternary Operator": {
    "prefix": "шартӣ",
    "body": [
      "${1:шарт} ? ${2:дуруст} : ${3:нодуруст}"
    ],
    "description": "Оператори шартӣ / Ternary operator"
  },
  "Destructuring Object": {
    "prefix": "тарқиш-объект",
    "body": [
      "собит { ${1:хосият1}, ${2:хосият2} } = ${3:объект};"
    ],
    "description": "Тарқиши объект / Object destructuring"
  },
  "Destructuring Array": {
    "prefix": "тарқиш-массив",
    "body": [
      "собит [${1:элемент1}, ${2:элемент2}] = ${3:массив};"
    ],
    "description": "Тарқиши массив / Array destructuring"
  },
  "Spread Operator": {
    "prefix": "пахш",
    "body": [
      "...${1:массив}"
    ],
    "description": "Оператори пахш / Spread operator"
  },
  "Rest Parameters": {
    "prefix": "остӣ",
    "body": [
      "функсия ${1:ном}(...${2:параметрҳо}: ${3:рақам}[]) {",
      "\t${4:// Коди функсия}",
      "}"
    ],
    "description": "Параметрҳои остӣ / Rest parameters"
  },
  "Enum Declaration": {
    "prefix": "рӯйхатӣ",
    "body": [
      "рӯйхатӣ ${1:НомиРӯйхатӣ} {",
      "\t${2:ҚИМАТ1},",
      "\t${3:ҚИМАТ2},",
      "\t${4:ҚИМАТ3}",
      "}"
    ],
    "description": "Эълони рӯйхатӣ / Enum declaration"
  },
  "Readonly Property": {
    "prefix": "фақатхонӣ",
    "body": [
      "фақатхонӣ ${1:хосият}: ${2:навъ};"
    ],
    "description": "Хосияти фақатхонӣ / Readonly property"
  },
  "Type Alias Union": {
    "prefix": "навъ-ё",
    "body": [
      "навъ ${1:НомиНавъ} = ${2:Навъ1} | ${3:Навъ2};"
    ],
    "description": "Навъи иттиҳодӣ / Union type alias"
  },
  "Type Alias Intersection": {
    "prefix": "навъ-ва",
    "body": [
      "навъ ${1:НомиНавъ} = ${2:Навъ1} & ${3:Навъ2};"
    ],
    "description": "Навъи истироқӣ / Intersection type alias"
  },
  "Tuple Type": {
    "prefix": "ҷуфт",
    "body": [
      "тағйирёбанда ${1:ном}: [${2:сатр}, ${3:рақам}] = [${4:\"қимат\"}, ${5:0}];"
    ],
    "description": "Навъи ҷуфт / Tuple type"
  },
  "Optional Property": {
    "prefix": "ихтиёрӣ",
    "body": [
      "${1:хосият}?: ${2:навъ};"
    ],
    "description": "Хосияти ихтиёрӣ / Optional property"
  },
  "Generic Interface": {
    "prefix": "интерфейс<T>",
    "body": [
      "интерфейс ${1:НомиИнтерфейс}<${2:T}> {",
      "\t${3:хосият}: ${2:T};",
      "\t${4:метод}(${5:параметр}: ${2:T}): ${6:навъи_бозгашт};",
      "}"
    ],
    "description": "Интерфейси умумӣ / Generic interface"
  },
  "Promise Function": {
    "prefix": "ваъда",
    "body": [
      "ҳамзамон функсия ${1:ном}(): Ваъда<${2:навъ}> {",
      "\tбозгашт нав Ваъда((ҳал, рад) => {",
      "\t\t${3:// Коди асинхронӣ}",
      "\t\tҳал(${4:натиҷа});",
      "\t});",
      "}"
    ],
    "description": "Функсияи Ваъда / Promise function"
  },
  "Async Await": {
    "prefix": "ҳамзамон-интизор",
    "body": [
      "ҳамзамон функсия ${1:ном}() {",
      "\tкӯшиш {",
      "\t\tсобит ${2:натиҷа} = интизор ${3:амалиёт}();",
      "\t\tбозгашт ${2:натиҷа};",
      "\t} гирифтан (${4:хато}) {",
      "\t\tчоп.хато(${4:хато});",
      "\t}",
      "}"
    ],
    "description": "Ҳамзамон бо интизор / Async with await"
  }
}
