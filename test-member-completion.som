// Тест барои member completion

синф Ҳайвон {
    хосусӣ ном: сатр;

    конструктор(ном: сатр) {
        ин.ном = ном;
    }

    ҷамъиятӣ овоз_додан(): сатр {
        бозгашт "Садои умумӣ";
    }
}

синф Саг мерос Ҳайвон {
    ҷамъиятӣ овоз_додан(): сатр {
        бозгашт "Вақ-вақ!";
    }

    ҷамъиятӣ вақ_вақ(): сатр {
        бозгашт "Вақ-вақ, вақ-вақ!";
    }
}

тағйирёбанда саг = нав Саг("Рекс");
чоп.сабт(саг.);
